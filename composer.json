{
    "name": "aciety/material-icons",
    "type": "library",
    "license": "MIT",
    "description": "Material Icons in PHP as SVG Objects",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-simplexml": "*",
        "meyfa/php-svg": "^0.11.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "^9.5",
        "symfony/console": "^6.1",
        "symfony/filesystem": "^6.1",
        "symfony/http-client": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Aciety\\Component\\MaterialIcons\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aciety\\Component\\MaterialIcons\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
