{
    "name": "aciety/material-icons",
    "type": "library",
    "license": "MIT",
    "description": "Material Icons in PHP as SVG Objects",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-simplexml": "*",
        "meyfa/php-svg": "^0.15.0"
    },
    "require-dev": {
        "composer/composer": "^2.7.7",
        "friendsofphp/php-cs-fixer": "^3.59.3",
        "mikey179/vfsstream": "^1.6.11",
        "phpstan/phpstan": "^1.11.5",
        "phpstan/phpstan-deprecation-rules": "^1.2.0",
        "phpstan/phpstan-phpunit": "^1.4.0",
        "phpstan/phpstan-strict-rules": "^1.6.0",
        "phpunit/phpunit": "^11.2.5",
        "symfony/console": "^7.1.0",
        "symfony/filesystem": "^7.1.0",
        "symfony/http-client": "^7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Aciety\\Component\\MaterialIcons\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aciety\\Component\\MaterialIcons\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
